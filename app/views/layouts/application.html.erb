

<!DOCTYPE html>
<html class ='html'>
<head>
  <title>Accent Global</title>

  <meta name="viewport" content="width=device-width, initial-scale=1">


  <%= stylesheet_link_tag  'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'https://js.stripe.com/v2/', 'application', 'data-turbolinks-track' => true %>
  <%= javascript_tag "Stripe.publishableKey = '#{Rails.configuration.stripe[:publishable_key]}';", type: 'text/javascript' %>

   <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400;300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>

    <link href="https://gitcdn.github.io/bootstrap-toggle/2.2.0/css/bootstrap-toggle.min.css" rel="stylesheet">
    <script src="https://gitcdn.github.io/bootstrap-toggle/2.2.0/js/bootstrap-toggle.min.js"></script>

  
  <%= csrf_meta_tags %>
  

</head>
<body class = 'body'>

  <div id="header-wrap">


    <div class="menu">

      <!-- Menu icon -->
      
      <div class="icon-close">
        <img src="http://s3.amazonaws.com/codecademy-content/courses/ltp2/img/uber/close.png">
      </div>

      <!-- Menu -->
        <% if user_signed_in? %> 
                <ul>
                  <center>
                  <li><h2>@<%= current_user.first_name %><%= current_user.last_name %></h2></li> 
                  <hr style='width: 200px;color: black;'>
                  <% if admin_signed_in? %>
                    <li><%= link_to "Home", '#' %></li>
                    <li><%= link_to "Users", '#' %></li>
                    <li><%= link_to "History", '#' %></li>
                    <li><%= link_to "Log out",  destroy_user_session_path, method: :delete %></li>
                  <%else%>
                  <li><%= link_to "Dashboard", dashboard_path %></li>
                  <li><a href="sms:/* 6154842917 */&body=/* I'm Ready */">Request(Mobile Only)</a></li>
                  <li><%= link_to "Vault", 'http://althealth2.weebly.com/services.html'%></li> 
                  <li><%= link_to "Settings", edit_user_registration_path %></li> 
                  <li><%= link_to "Share", 'http://http://althealth2.weebly.com/share.html' %></li> 
                  <li><%= link_to "Support", 'http://althealth2.weebly.com/support' %></li> 
                  <li><%= link_to "Site Feedback", 'http://althealth2.weebly.com/support' %></li> 
                  <li><%= link_to "Log out",  destroy_user_session_path, method: :delete %></li> 
                  <%end%>
                </center>
                </ul>  
        
        <% else %> 
                <ul> 
                  <center>
                  <li><%= link_to "Login", new_user_session_path %></a></li> 
                  <li><%= link_to "Signup", new_user_registration_path %></a></li> 
                  <li><%= link_to "Home", root_path %></a></li> 
                  </center>
                </ul> 
        <% end %>
        
        <br><br>
        <center><p>Â© 2014-2015 Accent Global</p></center>
    </div>

    <!-- Main body -->
    
  
       <div class="icon-menu">
       <i class="fa fa-bars"></i>
        <span class="color">ACCENT</span>
      </div>


      
 

<div class = 'content'>
  <% flash.each do |name, msg| %>
     <%= content_tag(:div, msg, class: "alert alert-#{name}") %>
  <% end %>
<%= yield %>
</div>



  <script>
      var c=0, words=['Welcome','Bienvenido','Namaste','Welkom','Bienvenue'];


      function loop(){
        $('h2 span').delay(2000).fadeTo(800,0,function(){
           $(this).text( words[++c%words.length]).fadeTo(800,1,loop);
        });
      }

      loop();

  </script>





</body>
</html> 